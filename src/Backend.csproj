    <ImplicitUsings>enable</ImplicitUsings>
    <InvariantGlobalization>true</InvariantGlobalization>
    <UserSecretsId>ae37a7ba-0c7d-4745-aff7-41907f176456</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
